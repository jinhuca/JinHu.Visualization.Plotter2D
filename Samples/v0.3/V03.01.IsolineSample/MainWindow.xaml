<Window x:Class="IsolineSample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:jin="http://jinhuca.visualization.com/Plotter2D/1.0"
        Title="MainWindow">
    <Grid>
    <jin:Plotter x:Name="plotter">
      <jin:IsolineGraph x:Name="isolineGraph"/>
      <jin:IsolineTrackingGraph x:Name="trackingGraph"/>
      <jin:CursorCoordinateGraph/>
    </jin:Plotter>
    </Grid>
</Window>
